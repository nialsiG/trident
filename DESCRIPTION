Package: trident
Type: Package
Title: Dental Microwear Texture Analysis
Version: 1.3.2
Authors@R: c(
    person(given = "Ghislain", family = "Thiery", role = c("aut", "cre"), email = "ghislain.thiery@ntymail.com"),
